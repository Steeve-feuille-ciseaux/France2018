<form method="post" action="{{ path('app_pays_delete', {'id': pays.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce pays ?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ pays.id) }}">
    <button class="btn btn-danger">Supprimer</button>
</form>
